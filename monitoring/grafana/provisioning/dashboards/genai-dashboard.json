{
  "id": null,
  "uid": "genai-metrics-dashboard",
  "title": "GenAI Metrics Dashboard",
  "timezone": "browser",
  "schemaVersion": 41,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Generation Duration",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "genai_generation_duration_seconds",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Generation Errors",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "genai_generation_errors_total",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Files Ingested",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "genai_ingested_files_total",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Ingestion Errors",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "genai_ingestion_errors_total",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingestion Duration",
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "genai_ingestion_duration_seconds",
          "refId": "A"
        }
      ]
    }
